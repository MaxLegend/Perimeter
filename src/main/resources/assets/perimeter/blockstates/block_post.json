{
  "multipart": [
    {
      "when": {
        "state": "down"
      },
      "apply": {
        "model": "perimeter:block/post/post_down"
      }
    },
    {
      "when": {
        "state": "middle"
      },
      "apply": {
        "model": "perimeter:block/post/post_middle"
      }
    },
    {
      "when": {
        "state": "up"
      },
      "apply": {
        "model": "perimeter:block/post/post_up"
      }
    },
    {
      "when": {
        "state": "cap"
      },
      "apply": {
        "model": "perimeter:block/post/post_cap"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "perimeter:block/post/post_connect",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "perimeter:block/post/post_connect",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "perimeter:block/post/post_connect",
        "y": 0,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "perimeter:block/post/post_connect",
        "y": 90,
        "uvlock": true
      }
    }
  ]
}
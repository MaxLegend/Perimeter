{
  "multipart": [
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "perimeter:block/contactfence/center",
        "y": 0
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "perimeter:block/contactfence/center",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "perimeter:block/contactfence/center",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "perimeter:block/contactfence/center",
        "y": 270
      }
    },
    {
      "when": {
        "left": true,
        "facing": "south"
      },
      "apply": {
        "model": "perimeter:block/contactfence/left",
        "y": 0
      }
    },
    {
      "when": {
        "left": true,
        "facing": "north"
      },
      "apply": {
        "model": "perimeter:block/contactfence/left",
        "y": 180
      }
    },
